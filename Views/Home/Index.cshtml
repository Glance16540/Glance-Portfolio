function fizzbuzz() {
var firstnumber = Number(document.getElementById("num8").value);
var secondnumber = Number(document.getElementById("num9").value);
var result = " "
for (x = 1; x <= 100; x++) {

if (x % firstnumber == 0 && x % secondnumber == 0) {
result += " " + "fizzbuzz"
}
else if (x % firstnumber == 0) {
result += " " + "fizz";
}
else if (x % secondnumber == 0) {
result += " " + "buzz"
}
