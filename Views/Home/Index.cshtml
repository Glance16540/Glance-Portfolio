function Palindrome() {
var input = document.getElementById('str').value;
var changeCharacter = input.replace(/[^A-Z0-9]/i, "").toLowerCase();
var reverseWord = changeCharacter.split('').reverse().join('');
if (changeCharacter === reverseWord) {
result = "This is a Palindrome!";} 
else {
